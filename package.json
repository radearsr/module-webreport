{
  "name": "ModuleWebreports",
  "version": "1.0.0",
  "description": "Module webreports",
  "main": "./src/main.js",
  "scripts": {
    "tw-build:watch": "npx tailwindcss -i ./src/css/input.css -o ./public/styles.css --watch",
    "tw-build": "npx tailwindcss -i ./src/css/input.css -o ./public/styles.css",
    "start": "npx electron .",
    "dev": "nodemon --exec electron .",
    "build": "electron-builder -w",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Module Web Reports",
    "win": {
      "icon": "src/assets/report.png",
      "artifactName": "${productName} v${version}.${ext}"
    }
  },
  "keywords": [],
  "author": "RSR x GIAN",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "better-sqlite3": "^8.5.0",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.1.1",
    "puppeteer": "^20.8.2",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "autoprefixer": "^10.4.14",
    "electron": "^25.3.0",
    "electron-builder": "^24.6.3",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3"
  }
}
